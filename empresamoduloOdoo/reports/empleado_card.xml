<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <template id="report_empleado">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="emp">
                <t t-call="web.external_layout">
                    <h2>Reporte de empleado:</h2>
                    <t t-esc="emp.numEmpleado"/>
                    <t t-esc="emp.nombre"/>
                    <t t-esc="emp.apellidos"/>
                    <t t-esc="emp.edad"/>
                    <t t-esc="emp.fechaEntrada"/>
                    <t t-esc="emp.salario"/>
                </t>
>            </t>
        </t>
    </template>

</odoo>